banknotes = [5000, 2000, 1000, 500, 200, 100]

amount = int(input ("Введите сумму для снятия (кратную 100): "))

if amount % 100 != 0:
    print ("Сумма должна быть кратна 100!")
else:
    print (f"Сумма для выдачи: {amount} руб.")
    for nominal in banknotes:
        count = amount // nominal
        amount = amount % nominal
        if count > 0:
            print(f"{nominal} py6.: {count} шт.")
